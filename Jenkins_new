pipeline{

agent any


  stages{
 stage("from l0 to l1") {
 
    steps{
    script{
    def specificCause = currentBuild.getBuildCauses('hudson.model.Cause$UserIdCause')
if (specificCause) {
    println("Executed by user $specificCause.userName")
   
    echo "specificCause: $specificCause.userName"
    echo "specificCause: ${specificCause.userId}"
    def user_name = ${specificCause.userName}
}
when{
expression{user_name == "RaviShankar P"}
}

echo"Ravi u doneeeeeeee..."

    }
    
    }
           
        }
        
}
  
  }
